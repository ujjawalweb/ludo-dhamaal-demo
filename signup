<script type="text/javascript">
  var client_id = 99;
  var is_mobile  =  1;
  var is_email =    1;
  var BASE_URL = 'https://api.bopanel.xyz/index.php/Web_service/';
  var chk_sp =1;

  $(document).ready(function() {
reset_f();
var api_url=BASE_URL+"get_clientgameoption";

jQuery.ajax({
        type: 'POST',
        url: api_url,
        data: {
          client_id: client_id
        }, 
        dataType: 'json',
        cache: false,
        success: function(data) { 
          if(data.status==1){
            

          $.each(data.data, function(i, item){ 
            var game_option = item.game_option;

           
            if(game_option == "mobile_verification") {
            
               is_mobile = 0;
            chk_sp++;
           }else{

           } 

            if(game_option == "email_verification") {
              
              is_email = 0;
             chk_sp++;
           }
      }); 
          


          if(is_email == 1){
              $('.emailfileds').removeClass('hidden');
              //$('#emailotp').removeClass('hidden');


          }
          if(is_mobile == 1){

             $('.mobilefileds').removeClass('hidden');
               //$('#phnnumber').removeClass('hidden');

          }

          if(chk_sp == 3){

            $('#sign_up').removeAttr('id');
            $('#mobileotp').removeAttr('id');
            $('.finalsign_up').attr('id','mobileotp');


          }

          }else{
           alert("error");
          }
        },
    });
});

