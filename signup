   $(document).on('click','#sign_up', function(){
      $('.alert-danger1').html('');
    $('.alert-success1').html('');
      $('.alert-success1').hide();
    $('.alert-danger1').hide();
    
       var username  =  $("#username").val();
       var password=$("#password").val();
       var email=$("#email").val();
       var phnnumber=$("#phnnumber").val();
       var mailformat = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;
       var otp_type = $(this).attr('name');
       var clickonbtn = $(this);
       var promo_code   =  $('#checkin').val();
       var volid_promo = true;

      if (typeof $("#mobileotpbox").val() == undefined){var mobileotpbox = ''; }else{  var mobileotpbox = $("#mobileotpbox").val(); }
      if (typeof $("#emailotpbox").val() == undefined){var emailotpbox = ''; }else{  var emailotpbox = $("#emailotpbox").val(); }
     
    var user_id =  $('#user_id').val();


    // var volid_promo = true;
    // var device_type = "Browser";
    // var browser_type = getmybrower();
    // var longitude = "75.7870025634766";
    // var latitude = "26.9123992919922";
    // var city = "jaipur";
    // var state = "Rajasthan";
    //var state="is_web";
    //var imei="";
    //var device_id="";
    // var version_no = '{{ $version_app }}';
   //alert(user_name+"-"+txtPassword+"-"+txtConfirmPassword+"-"+mob_num+"-"+clientid);
   //alert(mail_otp+'--------'+final_otp);
    if(username=='')
       {
         $('.validationText4' ).html("Please Enter Username");
          $('#username').css('border','1px solid rgb(206, 24, 24)');
         return false;
      }else{
         $('.validationText4' ).html('');
         $('#username').css('border','1px solid rgb(206, 212, 218)');
      }
    
      if(username.length < 6)
         {
           $('.validationText4' ).html("Minimum 6 characters required");
            $('#username').css('border','1px solid rgb(206, 24, 24)');
           return false;
        }else{
           $('.validationText4' ).html('');
           $('#username').css('border','1px solid rgb(206, 212, 218)');
        }

    if(password=='')
       {
       
         $('.validationText4' ).html('');
        
         $('.validationTextp' ).html("Please Enter Password");
         $('#password').css('border','1px solid rgb(206, 24, 24)');
         return false;
      }else{
         $('.validationTextp' ).html('');
         $('#password').css('border','1px solid rgb(206, 212, 218)');
      }
    
      if(is_email === 1 && email==''){
       $('.validationText5' ).html("Please Enter Email");
        $('#email').css('border','1px solid rgb(206, 24, 24)');
        return false;
      } else {
       $('.validationText5' ).html("");
       $('#email').css('border','1px solid rgb(206, 212, 218)');
      }
    if(email!=''){
      if(email.match(mailformat)) {
        if(email.match(/\s/g)){
          $('.validationText5' ).html("Please Enter Valid Email");
          $('#email').css('border','1px solid rgb(206, 24, 24)');
          return false;
        }else{
        $('.validationText5' ).html("")
        $('#email').css('border','1px solid rgb(206, 212, 218)');
      }
      } else {
        $('.validationText5' ).html("Please Enter Valid Email");
        $('#email').css('border','1px solid rgb(206, 2, 24)');
        return false;
      }
    }
    
    if(is_mobile === 1 && phnnumber==''){
         $('.validationText2' ).html("Please Enter Mobile Number");
         $('#phnnumber').css('border','1px solid rgb(206, 24, 24)');
        return false;
      } else {
        $('.validationText2' ).html("")
        $('#phnnumber').css('border','1px solid rgb(206, 212, 218)');
      }
    

    if(phnnumber!='')
       {
        if($('#phnnumber').val().length != 10){
        $('.validationText1' ).html('');
         $('.validationText2' ).html("Please Enter Valid Mobile Number");
         $('#phnnumber').css('border','1px solid rgb(206, 24, 24)');
         return false;
       }else {
        $('.validationText2' ).html('');
        $('#phnnumber').css('border','1px solid rgb(206, 212, 218)');
      }
      } 

    if(phnnumber!='')
    {
        if(mobileotpbox=='')
        {
         $('.validationText2' ).html('');
         $('.mobileopp' ).html("Please Enter OTP");
             return false;
        }
    }
    if(email!='')
    {
      if(emailotpbox=='')
      {
       $('.mobileopp' ).html('');
       $('.mailopp' ).html("Please Enter OTP");
           return false;
      }
    }
    
    if($("#chb1").is(':checked') == false){
       $('.mailopp' ).html('');
          $(".validationText3").text("Please accept terms and condition").show();
      return false;
    }
   
    

    var api_url=BASE_URL+"confirmotp_v1";

     $('#sign_up').attr('disabled', 'disabled');
      $(".validationText3").text("").hide();
     
     
      $.ajax({
                  url: api_url,
                  method: 'post',
                  data: {
                  user_id : user_id,
                  username: username,
                  password:password,
                  phone_number:phnnumber,
                  otp:mobileotpbox,
                  mailotp:emailotpbox,
                  client_id:client_id,
                  promo_code,promo_code,
                  device_type:0,
                  browser_type:0,
                  longitude:0,
                  latitude:0,
                  version_no:0,
                  device_id:'',
                  is_autologin_user:"",
                  },
                  dataType: 'JSON',
                  cache: false,
                  async:false,

                  success: function(data){
                        var result=data;
                        console.log(result);
                        if(result.status=="1"){
                       
                          $('.alert-success1').show();
                            $('.alert-success1').append(result.msg);

                            window.location.href="main.html";
                              setTimeout(function() {
                                    $('.alert-success1').hide();
                                    //location.reload();
                              }, 2000);
                              
                        }else{
            $('#sign_up').removeAttr('disabled', 'disabled');
           
             $('.alert-danger1').show();
                $('.alert-danger1').append(result.msg);
              setTimeout(function() {
                    $('.alert-danger1').hide();
                                    
                              }, 1500);
                
                        }
                  },
                  error: function(data){

                      var errors = data.responseJSON.errors;
                    //  console.log(errors);

                      $.each(errors, function(key, value){

                        $('.alert-danger1').show();
                        $('.alert-danger1').append('<p>'+value+'</p>');
                      });
                  }

                  });
    
    
    
   });

function reset_f(){
  $("#username").removeAttr("style");
  $("#password").removeAttr("style");
  $("#phnnumber").removeAttr("style");
  $("#username").val('');
  $("#password").val('');
  $("#phnnumber").val("");
  $("#email").val("");
  $("#emailotpbox").val("");
  $("#mobileotpbox").val("");  
  $("#checkin").val(""); 
  $("#chb").prop("checked",false);
  $("#chb1").prop("checked",false);
  $("#checkin").addClass('hidden');  
}


</script>