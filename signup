$(document).on('click','#mobileotp,#emailotp', function(){

 var username  =  $("#username").val();
 var password=$("#password").val();
 var email=$("#email").val();
 var phnnumber=$("#phnnumber").val();
 var mailformat = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;
 var otp_type = $(this).attr('name');
 var clickonbtn = $(this);
 var promo_code   =  $('#checkin').val();
 var volid_promo = true;

    
 if(username=='')
       {
         $('.validationText4').html("Please Enter Username");
         $('#username').css('border','1px solid rgb(206, 24, 24)');
         return false;
      }
      if(username.length < 6)
       {
         $('.validationText4' ).html("Minimum 6 characters required");
          $('#username').css('border','1px solid rgb(206, 24, 24)');
         return false;
      }else{
         $('.validationText4' ).html('');
         $('#username').css('border','1px solid rgb(206, 212, 218)');
      }

    if(password=='')
       {
         $('.validationText4' ).html('');
        
         $('.validationTextp' ).html("Please Enter Password");
         $('#Password').css('border','1px solid rgb(206, 24, 24)');
         return false;
      }else{
         $('.validationTextp' ).html('');
         $('#Password').css('border','1px solid rgb(206, 212, 218)');
      }

if(email!=''){
      if(email.match(mailformat)) {
        if(email.match(/\s/g)){
          $('.validationTextp' ).html('');
          $('.validationText5' ).html("Please Enter Valid Email");
          $('#email').css('border','1px solid rgb(206, 24, 24)');
          return false;
        }else{
        $('.validationText5' ).html("")
        $('#email').css('border','1px solid rgb(206, 212, 218)');
      }
      } else {
        $('.validationText5' ).html("Please Enter Valid Email");
        $('#email').css('border','1px solid rgb(206, 2, 24)');
        return false;
      }
    }

      if(phnnumber!='')
       {
        if($('#phnnumber').val().length != 10){
        $('.validationText5' ).html('');
         $('.validationText2' ).html("Please Enter Valid Mobile Number");
         $('#phnnumber').css('border','1px solid rgb(206, 24, 24)');
         return false;
       }else {
        $('.validationText2' ).html('');
        $('#phnnumber').css('border','1px solid rgb(206, 212, 218)');
      }
      }

 

    var api_url=BASE_URL+"api_passenctypt";

     $.ajax({
                  url: api_url,
                  method: 'post',
                  data: {
          password:password
                   },
                  dataType: 'JSON',
                  cache: false,

                  success: function(data){
           // console.log(data);
           
                        var result=data;
                        console.log(result.user_pass);
                           passwd=result.user_pass;
                          //$('#encrptpass').val(passwd);
              
              get_excryptpass(username,password,phnnumber,client_id,passwd,email,clickonbtn,otp_type,user_id,promo_code);
                  }
              });


});



 function  get_excryptpass(username,password,phnnumber,client_id,passwd,email,clickonbtn,otp_type,user_id,promo_code)
 
 {
   $('.alert-danger1').html('');
    $('.alert-success1').html('');
   var  user_name =username;
   var txtPassword=password;
   var mob_num=phnnumber;
   var email_id=email;
   var clientid=client_id;
   var passwd=passwd;

   //alert(passwd);
   
   //console.log(passwd);

   if(email_id == ""){
  email_id="";
}

if(mob_num == ""){
  mob_num="";
}

   
   
   var api_url=BASE_URL+"web_registration_v1";
    
     clickonbtn.attr('disabled', 'disabled');
     $('#phnnumber').attr('disabled', 'disabled');
      $.ajax({
                  url: api_url,
                  method: 'post',
                  data: {
                  username: user_name,
                  password:passwd,
                  phone_number:mob_num,
                  pemail:email_id,
                  client_id:clientid,
                  otp_type:otp_type,
                  user_id:0,
                  promo_code:promo_code,
                  device_id:'',
                  agent_id:''
                  },
                  dataType: 'JSON',
                  cache: false,

                  success: function(data){
                        var result=data;
                        console.log(result);
                        if(result.status=="1"){
                             $("#user_id").val(data.user_id);
                
                              //$('#myModal').modal('toggle');
                              $('.alert-success1').show();
                                $('.alert-success1').append(result.msg);
                                  setTimeout(function() {
                                        $('.alert-success1').fadeOut();
                                        
                                  }, 1500);
                            
                              if(clickonbtn.data('type')=="email_data"){
                                   $("#emailotpbox").show();
                                        }
                             if(clickonbtn.data('type')=="mobile_data"){
                                   $("#mobileotpbox").show();
                                        }
                            //clickonbtn.removeAttr('disabled', 'disabled');

                            var left_time = 30;
                            if(clickonbtn.attr('name') != 'no_otp'){
                              clickonbtn.closest('.row_margin').find('.hideandshow').removeClass('hidden');
                              timeinerval=setInterval(function() {
                                left_time = left_time < 10 ? '0'+left_time: left_time;
                                clickonbtn.html("&ensp;&emsp;"+left_time+"&emsp;&ensp;");
                                left_time = parseInt(left_time)-1;
                                if(left_time == 0)
                                {  
                                   clickonbtn.html('Resend OTP');
                                   clickonbtn.css('font-size','14px');
                                   clickonbtn.removeAttr('disabled', 'disabled');
                                   $('#phnnumber').removeAttr('disabled', 'disabled');
                                   clearInterval(timeinerval);
                                }
                              }, 1000);
                            }else{
                               window.location.href="main.html";
                            }
                            $('#'+otp_type).val('1');

                            // var mobile_otp = $('#mobile_otp').val();
                            // var email_otp = $('#email_otp').val();
                            // if(is_mobile == 0 && mobile_otp == 1 &&  is_email == 0  && email_otp == 1){

                            // $('#final_submit').removeAttr('disabled', 'disabled');
                            // }else if(is_mobile == 0 && mobile_otp == 1){
                            //    $('#final_submit').removeAttr('disabled', 'disabled');
                            // }else if(is_email == 0  && email_otp == 1){ 
                            //      $('#final_submit').removeAttr('disabled', 'disabled');
                            // }else{
                            //    setTimeout(function() {
                            //   var current_method = '<?= $current_method ?>';
  
                            //   if (current_method != 'signup'){ 
                            //     login_passencript(user_name,passwd,'');
                            //   }else{
                            //     window.location.href = "{{ Config::get('constants.BASE_URL') }}"+"/registersuccess"; 
                            //   }
                            //         //location.reload();
                            //   }, 3000);
                            // }

                        }else{
              
                            
                            str = result.msg.replace(/,\s*$/, "");
                            str = str + '.';
                            if(str.search("username") > -1)
                            {
                              $('.validationText4').text(str)
                            }else if(str.search("email") > -1){
                              $('.validationText5' ).text(str)
                            }else if(str.search("Phone") > -1){
                              $('.validationText2').text(str)
                            }else{
                            $('.alert-danger1').show();
                            $('.alert-danger1').append(str);
                            setTimeout(function() {
                            $('.alert-danger1').fadeOut();      
                                            }, 1500);
                            }
                           clickonbtn.removeAttr('disabled', 'disabled');
                           $('#phnnumber').removeAttr('disabled', 'disabled');
                           
                        }


                  },
                  error: function(data){

                      var errors = data.responseJSON.errors;
                    //  console.log(errors);

                      $.each(errors, function(key, value){
                         clickonbtn.removeAttr('disabled', 'disabled');
                        $('.alert-danger1').show();
                        $('.alert-danger1').append('<p>'+value+'</p>');
                      });
                  }

                  });
   
 }



